{
  "expo": {
    "name": "Make It Animated",
    "slug": "make-it-animated",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-ios.png",
    "scheme": "make-it-animated",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.make-it-animated.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-android.png",
        "backgroundColor": "#131316"
      },
      "package": "com.makeitanimated.app",
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#131316"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cb26971b-9cd8-4f46-ba13-809821a5015e"
      }
    },
    "owner": "volodymyr_serbulenko"
  }
}
