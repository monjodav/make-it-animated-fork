{
  "expo": {
    "name": "Make It Animated",
    "slug": "make-it-animated",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-ios.png",
    "scheme": "make-it-animated",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.make-it-animated.app",
      "appleTeamId": "8Q6HG7RGQP"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-android.png",
        "backgroundColor": "#131316"
      },
      "package": "com.makeitanimated.app",
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#131316"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
